    skos:broader <http://linked.data.gov.au/def/tern-cv/2be2d5ce-ca9d-4dff-8e79-948983944f95> ;
    skos:definition "Physiologically distinct and independent plant." ;
    skos:example "An individual tree, an individual shrub, a single Licuala ramsayi individual" ;
